{
  "name": "@envygeeks.io/cdk",
  "private": true,
  "scripts": {
    "gc": "dotenvx run -f .env.local -- cdk --unstable=gc gc",
    "diff": "dotenvx run -f .env.local -- cdk diff --require-approval=never",
    "deploy:prod": "dotenvex -f .env.prod -- pnpm run --filter=cdk deploy Dev/* cdk deploy",
    "deploy:dev": "dotenvex -f .env.dev -- pnpm run --filter=cdk deploy Dev/* cdk deploy",
    "destroy": "dotenvx run -f .env.local -- cdk destroy --require-approval=never",
    "synth": "dotenvx run -f .env.local --  cdk synth --unstable=gc"
  },
  "devDependencies": {
    "aws-cdk": "~2.1003",
    "aws-cdk-lib": "~2.173",
    "cdk-nag": "^2.35.47",
    "constructs": "^10.4.2"
  }
}
